{
    "DOC_CLASS": "article",
    "DEF_FONT": "lmodern",
    "FONT_SIZE": 12,
    "MARGIN": "1in",
    "PAPER_SIZE": "a4paper",
    "MAKE_TITLE": true,
    "PACKAGES": [
            "[margin=<MARGIN>, <PAPER_SIZE>]{geometry}",
            "{indentfirst}",
            "{amsmath}",
            "{mathtools}",
            "{symbols}",
            "{footmisc}",
            "{gensymb}",
            "{xcolor}",
            "{caption}",
            "[colorlinks, allcolors=<LINK_COLORS>]{hyperref}"
        ],
    "SECTION_SIZES": {
            "main": 12,
            "sub": 12,
            "subsub": 12,
            "para": 12,
            "subpara": 12
        },
    "AUTHOR": "John Doe",
    "DATE": "<NOW>",
    "COLOR_LINKS": true,
    "LINK_COLORS": "BLUE"
}
