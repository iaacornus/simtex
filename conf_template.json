{
    "DEF_FONT": "lmodern",
    "PACKAGES": [
            "{symbols}",
            "[margin=<MARGIN>, <PAPER_SIZE>]{geometry}",
            "{amsmath}",
            "{footmisc}",
            "{gensymb}",
            "{xcolor}",
            "{indentfirst}",
            "{caption}",
            "[colorlinks, allcolors=<LINK_COLORS>]{hyperref}",
            "{mathtools}"
        ],
    "FONT_SIZE": 12,
    "MARGIN": "1in",
    "PAPER_SIZE": "a4paper",
    "SECTION_SIZES": {
            "main": 12,
            "sub": 12,
            "subsub": 12,
            "para": 12,
            "subpara": 12
        },
    "MAKE_TITLE_SIZES": "DEF",
    "DOC_CLASS": "ARTICLE",
    "MAKE_TITLE": true,
    "AUTHOR": "John Doe",
    "DATE": "NOW",
    "COLOR_LINKS": true,
    "LINK_COLORS": "BLUE"
}
